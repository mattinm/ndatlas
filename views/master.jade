- function geturl(s) { return url.resolve(urlbase, s); }

doctype html
html(lang="en")
    head
        meta(charset="utf-8")
        meta(name="viewport", content="width=device-width, initial-scale=1")
        title!= title
        link(href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css", rel="stylesheet", integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp", crossorigin="anonymous")
        link(href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css", rel="stylesheet", integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u", crossorigin="anonymous")
        link(href="#{geturl('css/font-awesome.min.css')}", rel="stylesheet")
        link(href="#{geturl('css/bootstrap-social.css')}", rel="stylesheet")
        link(href="#{geturl('css/master.css')}", rel="stylesheet")
        block theme
            link(rel="stylesheet" type="text/css" href="#{geturl('css/light_menu.css')}")
        block css
    body
        nav(class="navbar navbar-und navbar-fixed-top")
            div(class="container-fluid")
                div(class="navbar-header")
                    button(type="button", class="navbar-toggle collapsed", data-toggle="collapse", data-target=".navbar-collapse")
                        span(class="icon-bar")
                        span(class="icon-bar")
                        span(class="icon-bar")
                    a(class="navbar-brand", href="/") NORTH DAKOTA ATLAS
                div(class="collapse navbar-collapse")
                    ul(class="nav nav-tabs")
                        li(role="presentation")
                            a(href="#{geturl('welcome')}") Welcome
                        li(role="presentation")
                            a(href="#{geturl('students')}") Students
                        li(role="presentation")
                            a(href="#{geturl('maps')}") Maps
                        li(role="presentation")
                            a(href="#{geturl('presentation')}") Presentation

        block body
            div(id="mainBody")
                block main
            block footer

        script(src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js")
        script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js", integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa", crossorigin="anonymous")
        script(src="#{geturl('js/master.js')}")
        block js
